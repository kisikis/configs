! urxvt settings (rxvt-unicode)
urxvt.font: xft:Hack-Regular:pixelsize=14
urxvt.boldFont: xft:Hack-Bold:pixelsize=14
!urxvt.font: xft:Monospace:pixelsize=14
!urxvt.boldFont: xft:Monospace:pixelsize=14
urxvt.background: #111111
urxvt.foreground: #eeeeee
urxvt.scrollBar:		false
urxvt.scrollBar_right:	false
urxvt.borderLess:		true
urxvt.internalBorder: 3
urxvt.borderColor: #222222
urxvt.url-launcher: /usr/bin/firefox
urxvt.underlineURLs: true
urxvt.urlButton: 1
urxvt.letterSpace: -1
urxvt.perl-lib: /usr/lib/urxvt/perl/
urxvt.perl-ext-common: default,matcher
urxvt.matcher.button: 1
urxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&amp;@#-]*[\\w/-]

! Xft settings
Xft.antialias: true
Xft.hinting: full
Xft.hintstyle: hintfull
Xft.rgba: rgb
